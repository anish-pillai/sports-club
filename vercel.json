{
  "buildCommand": "prisma generate && next build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "env": {
    "NEXTAUTH_URL": "https://sports-club-pearl.vercel.app",
    "PRISMA_CLIENT_ENGINE_TYPE": "dataproxy"
  },
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "true"
    }
  }
}
